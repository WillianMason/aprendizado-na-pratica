Algoritmo "solucao-do-problema-consumo"

Var

   distancia, combustivel, consumo : real




Inicio

   escreva("Distancia percorrida: ")
   leia(distancia)
   
   escreva("Combustível gasto: ")
   leia(combustivel)
   
   consumo <- distancia / combustivel
   escreval("Consumo medio = ", consumo:6:3)




Fimalgoritmo